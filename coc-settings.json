{
  //# coc.nvim global configurations
  "codeLens.enable": true,
  "suggest.enablePreview": true,
  //"suggest.detailMaxLength": 100,
  //"suggest.maxPreviewWidth": 80,
  "suggest.asciiCharactersOnly": true,
  "suggest.removeDuplicateItems": true,
  //"suggest.acceptSuggestionOnCommitCharacter": true,
  "diagnostic.virtualText": true,
  //"suggest.maxCompleteItemCount": 14,
  //## coc-list
  "list.source.grep.command": "rg",
  "list.insertMappings": {
    "=": "do:toggle"
  },
  //## coc.nvim LSP configurations
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": [
        "c",
        "cpp",
        "cuda",
        "objc",
        "objcpp"
      ],
      "rootPatterns": [
        ".ccls-root",
        "compile_commands.json"
      ],
      "initializationOptions": {
        "cache": {
          "directory": ".ccls-cache"
        }
      }
    }
  },
  //## coc-flutter
  "flutter.lsp.initialization.closingLabels": true,
  //## coc-python
  "python.poetryPath": "poetry",
  "python.jediEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.mypyEnabled": true,
  "python.formatting.provider": "black",
  //## coc-pyright
  //"pyright.openFilesOnly": true,
  //"pyright.useLibraryCodeForTypes": true,
  //"pyright.disableLanguageServices": true,
  //## coc-java
  "java.trace.server": "verbose",
  "java.signatureHelp.enabled": true,
  "java.format.settings.url": "/home/l8ng/.config/coc/extensions/node_modules/redhat.java/eclipse-formatter.xml",
  //## coc-go
  "go.goplsPath": "~/LibHateRoot/go/bin/gopls",
  //## coc-snippets
  "snippets.snipmate.enable": true,
  "snippets.ultisnips.enable": false,
  "snippets.ultisnips.directories": [
    "UltiSnips",
    "gosnippets/UltiSnips"
  ],
  //## coc-json
  "json.schemas": [
    {
      "fileMatch": [
        ".vimspector.json"
      ],
      "url": "https://puremourning.github.io/vimspector/schema/vimspector.schema.json"
    },
    {
      "fileMatch": [
        ".gadgets.json",
        ".gadgets.d/*.json"
      ],
      "url": "https://puremourning.github.io/vimspector/schema/gadgets.schema.json"
    }
  ],
  //## coc-word
  "coc.source.word.filetypes": [
    "md"
  ],
  //## coc-todolist
  //## coc-yank
  "yank.enableCompletion": false,
  //## coc-pairs => jiangmiao/auto-pairs (*)
  "pairs.enableCharacters": [
    "(",
    "[",
    "{",
    "<",
    "'",
    "\"",
    "`"
  ],
  //## coc-explorer => preservim/nerdtree (*)
  "explorer.previewAction.onHover": false,
  "explorer.width": 30,
  "explorer.icon.enableNerdfont": true,
  "explorer.activeMode": true,
  "explorer.file.hiddenRules": {
    "extensions": [
      "o",
      "a",
      "obj",
      "pyc"
    ],
    "filenames": [],
    "patternMatches": [
      "^\\.",
      "__pycache__",
      "node_modules",
      "\\S.egg-info",
      "pip-\\S",
      "dist",
      "\\S.sqlite3",
      "\\S.db"
    ]
  },
  //## coc-git => lambdalisue/gina.vim (-), tpope/vim-fugitive (-)
  "git.addGBlameToBufferVar": true,
  "git.addGBlameToVirtualText": false,
  "git.enableGutters": true,
  "git.topRemovedSign.text": "‾",
  "git.removedSign.text": "_",
  "git.addedSign.text": "+",
  "git.changedSign.text": "~",
  "git.changeRemovedSign.text": "≃",
  //## coc-smartf => easymotion/vim-easymotion (-)
  "smartf.timeout": 2500,
  "smartf.charactersIgnoreByNavigator": [
    "o",
    "i",
    "a",
    "x",
    "d",
    "y"
  ],
  //# coc-template => aperezdc/vim-template (*)
  "template.username": "straydragon",
  "template.email": "straydragonv@gmail.com"
}
